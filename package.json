{
  "name": "apinegocio",
  "version": "1.0.0",
  "description": "This is a API logic bussines",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "ts-node src/server.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "typeorm": "ts-node --transpile-only ./node_modules/typeorm/cli.js -d src/configs/data-source.ts",
    "migration:generate": "npm run typeorm -- migration:generate ./src/utils/migrations/Init --pretty",
    "migrate": "npm run typeorm -- migration:run",
    "migrate:revert": "npm run typeorm -- migration:revert",
    "seed": "ts-node src/utils/seeds/index.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/miguelorg12/Api_Negocio_Rondines.git"
  },
  "author": "Miguel Villalpando",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/miguelorg12/Api_Negocio_Rondines/issues"
  },
  "homepage": "https://github.com/miguelorg12/Api_Negocio_Rondines#readme",
  "dependencies": {
    "dotenv": "^17.1.0",
    "express": "^5.1.0",
    "express-validator": "^7.2.1",
    "pg": "^8.16.3",
    "reflect-metadata": "^0.2.2",
    "typeorm": "^0.3.25"
  },
  "devDependencies": {
    "@types/express": "^5.0.3",
    "@types/node": "^24.0.13",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  }
}
