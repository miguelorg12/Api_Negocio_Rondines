{
  "name": "apinegocio",
  "version": "1.0.0",
  "description": "This is a API logic bussines",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "nodemon --watch src --exec ts-node -r tsconfig-paths/register src/server.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "start:prod": "NODE_ENV=production node dist/server.js",
    "typeorm": "ts-node -r tsconfig-paths/register --transpile-only ./node_modules/typeorm/cli.js -d src/configs/data-source.ts",
    "migration:generate": "npm run typeorm -- migration:generate ./src/utils/migrations/Init --pretty",
    "migrate": "npm run typeorm -- migration:run",
    "migrate:revert": "npm run typeorm -- migration:revert",
    "seed": "ts-node -r tsconfig-paths/register src/utils/seeds/index.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/miguelorg12/Api_Negocio_Rondines.git"
  },
  "author": "Miguel Villalpando",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/miguelorg12/Api_Negocio_Rondines/issues"
  },
  "homepage": "https://github.com/miguelorg12/Api_Negocio_Rondines#readme",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.856.0",
    "@aws-sdk/s3-request-presigner": "^3.856.0",
    "@types/multer": "^2.0.0",
    "bcryptjs": "^3.0.2",
    "class-transformer": "^0.5.1",
    "cors": "^2.8.5",
    "dotenv": "^17.1.0",
    "express": "^5.1.0",
    "express-validator": "^7.2.1",
    "multer": "^2.0.2",
    "pg": "^8.16.3",
    "reflect-metadata": "^0.2.2",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "typeorm": "^0.3.25"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/node": "^24.0.13",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.8",
    "nodemon": "^3.1.10",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.8.3"
  }
}
